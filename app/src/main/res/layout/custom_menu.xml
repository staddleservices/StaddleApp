                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:src="@drawable/ic_vector_right"
                    tools:ignore="ContentDescription" />

            </RelativeLayout>

            <RelativeLayout
